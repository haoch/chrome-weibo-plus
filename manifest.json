{
  "manifest_version": 2,
  "name": "WeiboPlus",
  "description": "A beautiful and productive Weibo extension, making weibo
  pages more beautiful, clear and powerful",
  "version": "0.0.1",
  "browser_action": {
	  "default_title":"WeiboPlus"
  },
  "content_scripts":[
	{
    "matches":["*://weibo.com/*/home*","*://www.weibo.com/*/home*"],
    "css":["style.css"],
    "js":["jquery.js","contentscript.js"]
	}
  ],
  "permissions":[
    "storage",
    "*://code.jquery.com/*"
  ]
}
